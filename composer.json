{
    "name": "phergie/phergie-irc-plugin-react-cfp",
    "description": "Phergie plugin for To list the currently open calls for papers",
    "keywords": [ "phergie", "irc", "bot", "plugin" ],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Emily Stamey",
            "email": "elstamey@gmail.com",
            "homepage": "http://phergie.org",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "elstamey@gmail.com",
        "irc": "irc://irc.freenode.net/phergie",
        "issues": "https://github.com/elstamey/phergie-irc-plugin-react-cfp",
        "source": "https://github.com/elstamey/phergie-irc-plugin-react-cfp"
    },
    "require": {
        "phergie/phergie-irc-bot-react": "~1",
        "phergie/phergie-irc-plugin-react-command": "~1.0"

    },
    "require-dev": {
        "phpunit/phpunit": "4.5.*",
        "phake/phake": "2.0.0-beta2"
    },
    "suggest": {
        "phergie/phergie-irc-plugin-react-commandhelp": "provides interactive help for commands"
    },
    "autoload": {
        "psr-4": {
            "Phergie\\Irc\\Plugin\\React\\Cfp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phergie\\Irc\\Tests\\Plugin\\React\\Cfp\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
